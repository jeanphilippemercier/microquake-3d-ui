{
  "private": true,
  "license": "UNLICENSED",
  "name": "paraview-quake",
  "version": "0.0.0-semantically-release",
  "description": "Quake Event viewer based on ParaView Web",
  "keywords": [
    "3d",
    "visualization",
    "quake",
    "paraview",
    "vtk"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.microquake.org/rio-tinto/microquake-3d-ui.git"
  },
  "bugs": {
    "url": "https://git.microquake.org/rio-tinto/microquake-3d-ui/issues"
  },
  "homepage": "https://git.microquake.org/rio-tinto/microquake-3d-ui#readme",
  "scripts": {
    "validate": "prettier --config ./prettier.config.js --list-different \"src/**/*.js\"",
    "reformat": "prettier --config ./prettier.config.js --write \"src/**/*.js\"",
    "build": "webpack --config .webpack.dev.config.js",
    "build:release": "webpack --config .webpack.prod.config.js -p",
    "version": "node ./build/writeVersion.js",
    "dev": "webpack --config .webpack.dev.config.js --watch",
    "commit": "git cz",
    "semantic-release": "semantic-release"
  },
  "author": "Kitware",
  "dependencies": {
    "vtk.js": "8.3.1",
    "paraviewweb": "3.2.6",
    "wslink": "0.1.8"
  },
  "devDependencies": {
    "kw-web-suite": "7.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
